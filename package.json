{
  "name": "rekordbox-flow-analyzer",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "description": "Foundation modules and desktop shell for Rekordbox Flow Analyzer",
  "scripts": {
    "cli": "node src/index.js",
    "dev:renderer": "vite",
    "build:renderer": "vite build",
    "smoke:electron": "npm run build:renderer && RBFA_SMOKE=1 electron .",
    "smoke:electron:safe": "npm run build:renderer && RBFA_SMOKE=1 ELECTRON_DISABLE_SANDBOX=1 electron .",
    "start:electron": "electron .",
    "start:electron:safe": "ELECTRON_DISABLE_GPU=1 electron .",
    "test": "node --test"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.0",
    "electron": "^37.2.0",
    "vite": "^7.1.4"
  }
}
